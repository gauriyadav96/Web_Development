

<%- include("partials/header"); -%>

<h1>Compose</h1>

<form action="/compose" method="post">
    <div class="form-group">
        <label for="title">Title</label>
        <input type="text" name="newTitle" autocomplete="off" class="form-control" >
    </div>
    <div class="form-group">
        <label for="post">Post</label>
        <textarea class="form-control" name="newPost" rows="5"></textarea>
    </div>
  <button type="submit" class="btn btn-primary btn-lg" name="publishBtn">Publish</button>
</form>



<%- include("partials/footer"); -%>
